  "author": "Metehan Kutlu <metehankutlu@outlook.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/metehankutlu/decky-save-manager/issues"
  },
  "homepage": "https://github.com/metehankutlu/decky-save-manager#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
