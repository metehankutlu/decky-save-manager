    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/react": "16.14.0",
    "@types/react-file-icon": "^1.0.1",
    "@types/uuid": "^9.0.1",
    "@types/webpack": "^5.28.1",
    "rollup": "^2.79.1",
